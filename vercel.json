{
  "functions": {
    "api/chat.py": {
      "runtime": "python3.9"
    },
    "api/health.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    { "src": "/chat", "dest": "/api/chat" },
    { "src": "/health", "dest": "/api/health" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
